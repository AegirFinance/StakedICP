{
  "canisters": {
    "governance": {
      "type": "custom",
      "wasm": "src/governance/governance.wasm",
      "candid": "src/governance/governance.did"
    },
    "ledger": {
      "type": "custom",
      "wasm": "src/ledger/ledger.wasm",
      "candid": "src/ledger/ledger.did"
    },
    "deposits": {
      "main": "src/deposits/deposits.mo",
      "type": "motoko"
    },
    "token": {
      "main": "src/DIP20/motoko/src/token.mo",
      "type": "motoko"
    },
    "metrics": {
      "dependencies": [
        "deposits",
        "ledger",
        "token"
      ],
      "main": "src/metrics/metrics.mo",
      "type": "motoko"
    },
    "website": {
      "dependencies": [
        "deposits",
        "token"
      ],
      "frontend": {
        "entrypoint": "dist/website/index.html"
      },
      "source": [
        "dist/website/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": "vessel sources"
    },
    "replica": {
      "subnet_type":"system"
    }
  },
  "dfx": "0.9.2",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": [
        "https://mainnet.dfinity.network"
      ],
      "type": "persistent"
    }
  },
  "version": 1
}
